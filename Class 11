FINAL KEYWORD(CLASS 11)


package room;

class A {
	final int noOfAlphabets = 26 ;
	final int noOfDigits;
	A(){
		noOfDigits = 10;
	}
	final void fun() {
		System.out.println("Hi, I'am a function in class A");
	}
}
final class B extends A{
	void fun() {
		System.out.println("I'am a function in B ovveriding fucntion in A");
	}
}

class C extends B{ //The type C cannot subclass the final class B
	
}

public class FinalStudy {
	public static void main(String[] args) {
		
	}

}
